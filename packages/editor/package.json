{
  "name": "editor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "preview": "serve ./dist/",
    "start": "concurrently -r \"yarn start:api\" \"vue-cli-service serve\"",
    "start:api": "cross-env TUTURE_PATH=fixtures NODE_ENV=development node api-server.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "@juggle/resize-observer": "^3.3.1",
    "@linusborg/vue-simple-portal": "^0.1.5",
    "@u3u/vue-hooks": "^2.0.1",
    "@vue/composition-api": "^1.0.0-rc.8",
    "@vueuse/core": "^4.8.1",
    "ant-design-vue": "^1.7.4",
    "core-js": "^3.6.5",
    "lodash": "^4.17.21",
    "lodash.some": "^4.6.0",
    "medium-zoom": "^1.0.6",
    "monaco-editor": "^0.25.0",
    "nano-assign": "^1.0.1",
    "prosemirror-commands": "^1.1.7",
    "prosemirror-keymap": "^1.1.4",
    "prosemirror-model": "^1.0.0",
    "prosemirror-state": "^1.3.4",
    "prosemirror-tables": "^1.1.1",
    "prosemirror-utils": "0.9.6",
    "prosemirror-view": "^1.18.0",
    "smooth-scroll-into-view-if-needed": "^1.1.32",
    "tiptap": "^1.32.1",
    "tiptap-extensions": "^1.35.1",
    "vue": "^2.6.11",
    "vue-demi": "^0.7.4",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "autoprefixer": "^9",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.3",
    "commitizen": "^3.0.5",
    "concurrently": "^6.1.0",
    "conventional-changelog": "^3.0.5",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.7.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.2.2",
    "mini-css-extract-plugin": "^1.4.1",
    "monaco-editor-webpack-plugin": "^4.0.0",
    "postcss": "^7",
    "postcss-loader": "4.0.3",
    "prettier": "^1.16.3",
    "sass": "^1.32.8",
    "sass-loader": "^10",
    "serve": "^11.3.2",
    "standard-version": "^4.4.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.0.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "vite-plugin-importer": "^0.2.1",
    "vue-cli-plugin-vite": "~1.0.0-rc.3",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off",
      "no-fallthrough": "off",
      "no-debugger": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
