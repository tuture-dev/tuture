    "chalk": "^2.4.2",
    "editure": "^0.1.2",
    "editure-constants": "^0.1.2",
    "fs-extra": "^7.0.0",
    "get-port": "^5.1.1",
    "inquirer": "^6.0.0",
    "micromatch": "^3.1.10",
    "open": "^6.4.0",
    "prosemirror-markdown": "1.5.1",
