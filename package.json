{
  "name": "tuture",
  "version": "1.1.0",
  "preferGlobal": true,
  "description": "Tuture magically turns your repositories into juicy tutorials.",
  "license": "MIT",
  "main": "bin/tuture-server",
  "keywords": [
    "tutorial",
    "step-by-step",
    "learn-by-doing",
    "git"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tutureproject/tuture.git"
  },
  "bugs": {
    "url": "https://github.com/tutureproject/tuture/issues"
  },
  "homepage": "https://github.com/tutureproject/tuture#readme",
  "bin": {
    "tuture-server": "bin/tuture-server"
  },
  "files": [
    "/bin",
    "/dist"
  ],
  "scripts": {
    "start": "TUTURE_PATH=example/en webpack -w --config webpack.dev.js",
    "start:cn": "TUTURE_PATH=example/cn webpack -w --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "prepublishOnly": "webpack --config webpack.prod.js",
    "test": "jest",
    "lint": "tslint 'src/**/*.{ts,tsx,js}' --project tsconfig.json",
    "precommit": "npm run lint && lint-staged"
  },
  "lint-staged": {
    "./**/*.{ts,tsx,js,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "express": "^4.16.3",
    "handlebars": "^4.0.11",
    "highlight.js": "^9.12.0",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.12.0",
    "marked": "^0.4.0",
    "morgan": "^1.9.0",
    "multer": "^1.3.1",
    "opn": "^5.3.0",
    "rc-upload": "^2.5.1",
    "react": "^16.3.2",
    "react-autosize-textarea": "^4.0.0",
    "react-dom": "^16.3.2",
    "serve-favicon": "^2.5.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "styled-components": "^3.3.2",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.4",
    "@types/express": "^4.16.0",
    "@types/handlebars": "^4.0.39",
    "@types/highlight.js": "^9.12.3",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/js-yaml": "^3.11.2",
    "@types/marked": "^0.4.0",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.7",
    "@types/opn": "^5.1.0",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "@types/serve-favicon": "^2.2.30",
    "@types/socket.io": "^1.4.36",
    "@types/socket.io-client": "^1.4.32",
    "clean-webpack-plugin": "^0.1.19",
    "cross-spawn": "^6.0.5",
    "fork-ts-checker-webpack-plugin": "^0.4.7",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.13.6",
    "svg-sprite-loader": "^3.8.0",
    "ts-loader": "^4.4.1",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.9.1",
    "typescript-plugin-styled-components": "^0.0.6",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4",
    "webpack-shell-plugin": "^0.5.0"
  }
}
