{
  "name": "tuture",
  "version": "2.1.0",
  "description": "Tuture magically turns your repositories into juicy tutorials.",
  "main": "index.ts",
  "preferGlobal": true,
  "keywords": [
    "learn-by-doing",
    "learn-by-examples",
    "hands-on",
    "tutorial",
    "diff-viewer",
    "git"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint:cli": "eslint . --ext .ts",
    "lint:editor": "eslint editor-next",
    "test": "rimraf build && tsc && jest",
    "build:editor": "cd editor && yarn && npm run build",
    "build:cli": "rimraf build && tsc",
    "build": "npm run build:editor && npm run build:cli && cp -r editor/build build/editor",
    "pkg": "./scripts/pack.sh",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tuture-dev/tuture.git"
  },
  "license": "MIT",
  "bin": {
    "tuture": "./bin/run"
  },
  "bugs": {
    "url": "https://github.com/tuture-dev/tuture/issues"
  },
  "homepage": "https://github.com/tuture-dev/tuture#readme",
  "files": [
    "/bin",
    "/build"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": [
      "pretty-quick --staged",
      "eslint . --fix"
    ]
  },
  "dependencies": {
    "@oclif/command": "^1.4.36",
    "@oclif/config": "^1.6.33",
    "@oclif/plugin-help": "^2.0.5",
    "chalk": "^2.4.2",
    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
    "globby": "^8.0.1",
    "inquirer": "^6.0.0",
    "js-yaml": "^3.13.1",
    "lodash.zip": "^4.2.0",
    "micromatch": "^3.1.10",
    "morgan": "^1.9.1",
    "multer": "^1.3.1",
    "open": "^6.4.0",
    "parse-diff": "^0.4.2",
    "rc": "^1.2.8",
    "request": "^2.88.0",
    "simple-git": "^1.129.0",
    "socket.io": "^2.1.1",
    "tmp": "^0.0.33",
    "which": "^1.3.1",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/fs-extra": "^5.0.4",
    "@types/inquirer": "^0.0.42",
    "@types/jest": "^23.3.1",
    "@types/js-yaml": "^3.11.2",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.zip": "^4.2.4",
    "@types/micromatch": "^3.1.0",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.7",
    "@types/rc": "^1.1.0",
    "@types/request": "^2.47.1",
    "@types/socket.io": "^1.4.38",
    "@types/tmp": "^0.0.33",
    "@types/which": "^1.3.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^4.2.3",
    "jest": "^24.9.0",
    "lint-staged": "^10.0.7",
    "lodash.clonedeep": "^4.5.0",
    "pkg": "^4.4.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.1.0",
    "typescript": "~3.6.4"
  },
  "types": "lib/index.d.ts",
  "oclif": {
    "commands": "./build/commands",
    "bin": "tuture",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "roots": [
      "test"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "build/**/*.ts"
    ],
    "testURL": "http://localhost/",
    "verbose": true
  },
  "pkg": {
    "scripts": "bin/run",
    "assets": "build/**/*"
  }
}
